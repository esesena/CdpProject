@model CDP.Models.Semente

@{
    ViewData["Title"] = "Detalhes";
}

<div class="container-fluid px-4">
    <div class="row border-bottom mt-2 md-4">
        <div class="col-lg-10 col-sm-6">
            <h2 class="" style="font-weight:bolder">
                <i class="bi bi-person-lines-fill m-1"></i>@ViewData["Title"]
            </h2>
        </div>
  </div>
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Cultura)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Cultura)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Descricao)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Descricao)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.HabitoCrescimento)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.HabitoCrescimento)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Altura)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Altura)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Floracao)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Floracao)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Resistencia)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Resistencia)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.GrupoMaturacao)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.GrupoMaturacao)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.ConsumoSementes)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ConsumoSementes)
        </dd>
    </dl>
    <div class="flex-column mt-4">
        <a asp-action="Edit" asp-route-id="@Model?.Id" class="btn btn-primary">Editar</a>
        <a asp-action="Index" class="btn btn-primary">Voltar</a>
    </div>
</div>
@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}