@model CDP.Models.Semente

@{
    ViewData["Title"] = "Delete";
}

<div class="container-fluid px-4">
    <div class="row border-bottom mt-2 md-4">
        <div class="col-lg-10 col-sm-6">
            <h2 class="" style="font-weight:bolder">
                <i class="bi bi-person-dash m-1"></i>@ViewData["Title"]
            </h2>
        </div>
    </div>
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Cultura)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Cultura)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Descricao)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Descricao)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.HabitoCrescimento)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.HabitoCrescimento)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Altura)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Altura)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Floracao)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Floracao)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Resistencia)
